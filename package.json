{
  "scripts": {
    "build": "rm -rf dist && pnpm build:es6 && pnpm build:cjs && pnpm build:types",
    "build:cjs": "swc src --out-dir dist/cjs --source-maps --config module.type=commonjs",
    "build:es6": "swc src --out-dir dist/es6 --source-maps --config module.type=es6",
    "build:types": "tsc --project tsconfig.dist.json",
    "fix": "find ./src -type f -exec sed -i -z 's/{\n/{/g' {} \\; && eslint --max-warnings=0 . --fix",
    "lint": "eslint --max-warnings=0 .",
    "pre-push": "pnpm pre-push:dirty && pnpm publish --dry-run",
    "pre-push:dirty": "pnpm install && pnpm build && pnpm lint && pnpm test",
    "test:codegen-app-nextjs": "cd test/codegen-app-nextjs && pnpm test",
    "test:codegen-stack-tests": "echo b",
    "test:treeshaking-nextjs": "echo c",
    "test:unit-test": "echo d",
    "gentest": "node dist/cjs/gentest.js",
    "test": "pnpm test:unit-tests && pnpm test:codegen-app-nextjs && pnpm test:codegen-stack-tests && pnpm test:treeshaking-nextjs"
  },
  "sideEffects": false,
  "name": "bxb",
  "repository": "https://github.com/aabccd021/bxb",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/es6/index.js"
    },
    "./next": {
      "require": "./dist/cjs/next.js"
    },
    "./dist/cjs/test": {
      "require": "./dist/cjs/test/index.js",
      "import": "./dist/es6/test/index.js"
    }
  },
  "files": [
    "dist",
    "public"
  ],
  "bin": "./dist/cjs/bin.js",
  "dependencies": {
    "@morphic-ts/batteries": "^3.0.0",
    "@morphic-ts/summoners": "^3.0.0",
    "@types/valid-data-url": "^2.0.0",
    "fp-ts": "^2.13.1",
    "fp-ts-std": "^0.15.1",
    "io-ts": "^2.2.19",
    "make-union-morphic-ts": "^1.0.7",
    "next": "^13.0.4",
    "node-fetch": "^3.3.0",
    "ts-essentials": "^9.3.0",
    "ts-pattern": "^4.0.6",
    "valid-data-url": "^4.0.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.19",
    "eslint": "^8.28.0",
    "happy-dom": "^7.7.0",
    "nazna": "^1.14.2",
    "pnpm": "^7.17.0",
    "semantic-release": "^19.0.5",
    "typescript": "^4.9.4",
    "vitest": "^0.24.5"
  }
}
