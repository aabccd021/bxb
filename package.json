{
  "scripts": {
    "build": "rm -rf dist && pnpm build:es6 && pnpm build:cjs && pnpm build:types",
    "build:cjs": "swc src --out-dir dist/cjs --source-maps --config module.type=commonjs",
    "build:es6": "swc src --out-dir dist/es6 --source-maps",
    "build:types": "tsc",
    "lint": "eslint . --max-warnings 0 --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix",
    "test-unit": "exec-main ./test/unit/index.test",
    "test-app-nextjs-treeshaking-capability": "exec-main ./test/app-nextjs-treeshaking-capability/index.test"
  },
  "sideEffects": false,
  "name": "bxb",
  "repository": "https://github.com/aabccd021/bxb",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es6/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "./dist/cjs/test": {
      "require": "./dist/cjs/test/index.js",
      "types": "./dist/cjs/test/index.d.ts"
    },
    "./dist/es6/test": {
      "import": "./dist/es6/test/index.js"
    },
    "./dist/cjs/scripts": {
      "require": "./dist/cjs/scripts/index.js",
      "types": "./dist/cjs/scripts/index.d.ts"
    },
    "./dist/es6/scripts": {
      "import": "./dist/es6/scripts/index.js",
      "types": "./dist/cjs/scripts/index.d.ts"
    },
    "./dist/cjs/next": {
      "require": "./dist/cjs/next.js"
    },
    "./dist/es6/next": {
      "import": "./dist/es6/next.js"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "eslintConfig": {
    "extends": "chimeraant",
    "overrides": [
      {
        "files": "src/test/**/*.ts",
        "rules": {
          "max-len": [
            "error",
            {
              "code": 100,
              "ignoreUrls": true,
              "ignorePattern": "(^export|^\\s*readonly|^import|^\\s*name: ')"
            }
          ]
        }
      }
    ]
  },
  "dependencies": {
    "fp-ts": "^2.13.1",
    "fp-ts-node": "^6.0.0",
    "io-ts": "^2.2.19",
    "next": "^13.0.4",
    "node-fetch": "^3.3.0",
    "ts-essentials": "^9.3.0",
    "ts-pattern": "^4.0.6",
    "valid-data-url": "^4.0.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27",
    "@types/node": "^18.11.18",
    "@types/valid-data-url": "^2.0.0",
    "c8": "^7.12.0",
    "eslint": "^8.28.0",
    "eslint-config-chimeraant": "^1.2.6",
    "exec-main": "^1.0.2",
    "pnpm": "^7.17.0",
    "pure-test": "^1.0.6",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
