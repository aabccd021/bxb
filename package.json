{
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.8",
    "eslint": "^8.25.0",
    "nazna": "^1.13.19",
    "pnpm": "^7.13.4",
    "semantic-release": "^19.0.5",
    "typescript": "^4.8.4",
    "vitest": "^0.24.1"
  },
  "scripts": {
    "build": "rm -rf dist && pnpm build:es6 && pnpm build:cjs && pnpm build:types && nazna build cli",
    "build:cjs": "swc src --out-dir dist/cjs --source-maps --config module.type=commonjs",
    "build:es6": "swc src --out-dir dist/es6 --source-maps",
    "build:types": "tsc --project tsconfig.dist.json && cp -R dist/types/. dist/cjs && cp -R dist/types/. dist/es6",
    "fix": "eslint --max-warnings=0 --ext .ts . --fix",
    "lint": "eslint --max-warnings=0 --ext .ts .",
    "pre-push": "pnpm pre-push:dirty && pnpm publish --dry-run",
    "pre-push:dirty": "pnpm install && pnpm build && pnpm lint && pnpm test",
    "test": "cd e2e/nextjs && pnpm install && pnpm test"
  },
  "sideEffects": false,
  "name": "masmott",
  "repository": "https://github.com/aabccd021/masmott",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/es6/index.js"
    },
    "./dist/es6/mock": {
      "import": "./dist/es6/mock.js"
    },
    "./dist/es6/next": {
      "require": "./dist/cjs/next.js"
    }
  },
  "files": [
    "dist",
    "public"
  ],
  "bin": "./dist/cli.js",
  "dependencies": {
    "@morphic-ts/batteries": "^3.0.0",
    "@morphic-ts/summoners": "^3.0.0",
    "fp-ts": "^2.13.1",
    "make-union-morphic-ts": "^1.0.7",
    "next": "^13.0.3"
  }
}
