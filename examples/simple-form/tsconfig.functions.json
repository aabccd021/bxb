{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"outDir": ".functions",
		"sourceMap": true,
		"strict": true,
		"target": "es2017",
		"allowJs": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"compileOnSave": true,
	"include": [
		"functions"
	]
}