{
  "scripts": {
    "build": "tsup",
    "do": "sh -c 'ts-node --compilerOptions \"{\\\"isolatedModules\\\": false}\" --eval \"import(\\\"$0\\\").then(a => a.main());\"'",
    "lint": "eslint --max-warnings=0 . --ignore-path .gitignore",
    "lint:fix": "eslint --max-warnings=0 . --fix",
    "lint:fix:compress": "fd --extension .ts --exec sed -i -z 's/{\\n/{/g' {} \\; && pnpm fix",
    "test": "c8 pnpm do ./test/unit/index.test",
    "test-codegen-custom": "echo b",
    "test-app-nextjs-treeshaking-capability": "vitest --run test/app-nextjs-treeshaking-capability",
    "test-app-nextjs-treeshaking-stack": "vitest --run test/app-nextjs-treeshaking-stack"
  },
  "sideEffects": false,
  "name": "bxb",
  "repository": "https://github.com/aabccd021/bxb",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/test": {
      "require": "./dist/test.js",
      "import": "./dist/test.mjs",
      "types": "./dist/test.ts"
    },
    "./dist/next": {
      "require": "./dist/next.js",
      "import": "./dist/next.mjs",
      "types": "./dist/next.d.ts"
    }
  },
  "tsup": {
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "entry": {
      "index": "src/index.ts",
      "test": "src/test/index.ts",
      "next": "src/next.ts"
    },
    "sourcemap": true,
    "clean": true
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "eslintConfig": {
    "ignorePatterns": ["dist", "node_modules"],
    "extends": "chimeraant"
  },
  "dependencies": {
    "fp-ts": "^2.13.1",
    "fp-ts-node": "^6.0.0",
    "io-ts": "^2.2.19",
    "next": "^13.0.4",
    "node-fetch": "^3.3.0",
    "ts-essentials": "^9.3.0",
    "ts-pattern": "^4.0.6",
    "valid-data-url": "^4.0.1"
  },
  "devDependencies": {
    "@types/valid-data-url": "^2.0.0",
    "@vitest/coverage-c8": "^0.26.2",
    "c8": "^7.12.0",
    "eslint": "^8.28.0",
    "eslint-config-chimeraant": "^1.2.5",
    "pnpm": "^7.17.0",
    "pure-test": "^1.0.4",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
