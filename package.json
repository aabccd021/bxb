{
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.8",
    "eslint": "^8.25.0",
    "nazna": "^1.13.19",
    "pnpm": "^7.13.4",
    "semantic-release": "^19.0.5",
    "serve": "^14.1.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.4",
    "vitest": "^0.24.1"
  },
  "scripts": {
    "build": "pnpm build:es6 && pnpm build:cjs && pnpm build:types && nazna build cli",
    "build:cjs": "swc src --out-dir dist/cjs --source-maps --config module.type=commonjs",
    "build:es6": "swc src --out-dir dist/es6 --source-maps",
    "build:types": "tsc --project tsconfig.dist.json",
    "codegen": "playwright codegen localhost:3000 --color-scheme dark",
    "fix": "eslint --max-warnings=0 --ext .ts . --fix",
    "lint": "eslint --max-warnings=0 --ext .ts .",
    "pre-push": "pnpm pre-push:dirty && pnpm publish --dry-run",
    "pre-push:dirty": "pnpm install && pnpm build && pnpm lint && pnpm test",
    "start": "serve .",
    "start:codegen": "start-test 3000 codegen",
    "test": "playwright test"
  },
  "name": "masmott",
  "repository": "https://github.com/aabccd021/masmott",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es6/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es6/index.js"
  },
  "files": [
    "dist",
    "public"
  ],
  "bin": "./dist/cli.js",
  "dependencies": {
    "@morphic-ts/batteries": "^3.0.0",
    "@morphic-ts/summoners": "^3.0.0",
    "fp-ts": "^2.13.1",
    "make-union-morphic-ts": "^1.0.7"
  }
}
