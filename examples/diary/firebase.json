{
  "emulators": {
    "firestore": {
      "port": 8080
    },
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  },
  "functions": {
    "ignore": [
      ".gitignore",
      ".masmott",
      ".next",
      "cypress",
      "firestore-debug.log",
      "masmott.ts",
      "node_modules",
      "package.json",
      "pages",
      "test",
      "tsconfig.json",
      "web",
      "yarn.lock"
    ],
    "source": "."
  },
  "hosting": {
    "cleanUrls": true,
    "ignore": [
      ".gitignore",
      ".masmott",
      ".next",
      "cypress",
      "firestore-debug.log",
      "masmott.ts",
      "node_modules",
      "package.json",
      "pages",
      "test",
      "tsconfig.json",
      "web",
      "yarn.lock"
    ],
    "public": "public",
    "rewrites": [
      {
        "function": "nextjs",
        "source": "**"
      }
    ]
  }
}