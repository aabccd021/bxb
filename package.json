{
  "name": "masmott",
  "version": "0.0.0-semantic-release",
  "main": "./dist/masmott.umd.js",
  "exports": {
    ".": {
      "import": "./dist/masmott.mjs",
      "require": "./dist/masmott.umd.js"
    }
  },
  "module": "./dist/masmott.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "lint": "format-package --check && eslint --max-warnings=0 --ext .ts .",
    "test": "vitest --coverage"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": [
        "**/tsconfig.**"
      ]
    },
    "overrides": [
      {
        "files": [
          "test/**/*.test.ts"
        ],
        "rules": {
          "functional/no-expression-statement": "off",
          "functional/no-return-void": "off"
        }
      }
    ],
    "extends": [
      "aabccd021"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "fp-ts": "^2.12.1",
    "io-ts": "^2.2.17",
    "make-struct-ts": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "c8": "^7.12.0",
    "eslint": "^8.19.0",
    "eslint-config-aabccd021": "^1.0.4",
    "eslint-plugin-fp-ts": "^0.3.2",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "format-package": "^7.0.0",
    "happy-dom": "^6.0.4",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "typescript-language-server": "^0.11.2",
    "vite": "^3.0.2",
    "vitest": "^0.18.0",
    "vue-tsc": "^0.39.0"
  }
}
